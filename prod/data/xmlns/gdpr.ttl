  rdfs:comment "Lijst met gevoelige gegevenscategorieën = ['A4', 'F1', 'J5','M1', 'M2', 'M3', 'M4', 'M5', 'O6', 'Q', 'R', 'S1', 'S2', 'T1', 'U']"@nl,
    "List of sensitive data categories = ['A4', 'F1', 'J5','M1', 'M2', 'M3', 'M4', 'M5', 'O6', 'Q', 'R', 'S1', 'S2', 'T1', 'U']"@en,
